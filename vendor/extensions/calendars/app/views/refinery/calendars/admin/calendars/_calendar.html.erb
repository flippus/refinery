<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(calendar) -%>">
  <span class='title'>
    <%= calendar.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.calendars_calendar_path(calendar),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_calendars_admin_calendar_path(calendar),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.calendars_admin_calendar_path(calendar),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => calendar.name),
        :method => :delete %>
  </span>
</li>